{
  "name": "testimora-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "next lint --fix",
    "cli-ops": "pnpm tsx cli/index.ts",
    "db:generate": "pnpm cli-ops drizzle:generate",
    "db:push": "pnpm cli-ops drizzle:push",
    "db:pull": "pnpm cli-ops drizzle:pull",
    "db:check": "pnpm cli-ops drizzle:check",
    "db:up": "pnpm cli-ops drizzle:up",
    "db:studio": "pnpm cli-ops drizzle:studio",
    "db:migrate": "pnpm cli-ops drizzle:migrate",
    "db:reset": "pnpm cli-ops drizzle:reset",
    "auth:generate": "pnpm cli-ops auth:generate",
    "auth:migrate": "pnpm cli-ops auth:migrate",
    "db:init": "pnpm tsx cli/index.ts drizzle:init",
    "init:better-auth:generate": "npx @better-auth/cli generate --config=cli/better-auth/auth.ts --output=cli/better-auth/output/auth-schema.ts",
    "test": "export PLAYWRIGHT=True && pnpm exec playwright test",
    "clean:all": "rm -f pnpm-lock.yaml && pnpm store prune && find . -name '.turbo' -type d -exec rm -rf {} + && find . -name '.next' -type d -exec rm -rf {} + && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "reinstall": "rm -f pnpm-lock.yaml && pnpm store prune && find . -name '.turbo' -type d -exec rm -rf {} + && find . -name '.next' -type d -exec rm -rf {} + && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && pnpm install",
    "git:init": "sh -c 'git add . && read -p \"Commit message: \" msg && git commit -m \"$msg\" && git remote -v && git config http.postBuffer 524288000 && git push -u origin main'",
    "analyze": "ANALYZE=true next build && open .next/analyze/client.html"
  },
  "browserslist": {
    "production": [
      "chrome >= 100",
      "edge >= 100",
      "firefox >= 100",
      "safari >= 15.4",
      "ios_saf >= 15.4",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@vercel/speed-insights": "^1.2.0",
    "better-auth": "^1.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.3",
    "lucide-react": "^0.536.0",
    "next": "15.4.5",
    "next-intl": "^4.3.4",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.9",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "prettier": "^3.6.2",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.2"
  }
}
